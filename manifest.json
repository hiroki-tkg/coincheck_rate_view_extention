{
  "manifest_version": 2,
  "name": "How much I bought at Coincheck",
  "version": "1.0",

  // Content scriptは、webページ内のhtml要素を取得できるが、変数は取得できない。
  // Content scriptはChromeの一部のAPIしかアクセスできない。
  // その時は、Event Pageを使って、アクセスする。
  "content_scripts": [ {
    "matches": [ 
      "<all_urls>"
      // ,"https://coincheck.jp/*"
    ],
    "js": [ "content.js" ]
  }]
}